class Solution {
public:
    void reverseString(vector<char>& s) {
        int i = 0;
        
        if (s.size() <= 1)
        {
            return;
        }
        
        for (i = 0; i <= (s.size() - 1)/2; i++)
        {
            swap(s.at(i), s.at(s.size()-1-i));
        } // end of for loop
    } // end of func
};
